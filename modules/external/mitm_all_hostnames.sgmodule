#!name=External: MITM All Hostnames
#!desc=Perform MITM on all hostnames with port 443, except those for Apple and other common sites which can't be inspected. You still need to configure a CA certificate and enable the main switch of MITM.
#!category=External

[MITM]
hostname = -*icloud*, -*.mzstatic.com, -*.facebook.com, -*.instagram.com, -*.twitter.com, -*dropbox*, -*apple*, -*.amazonaws.com, -<ip-address>, *